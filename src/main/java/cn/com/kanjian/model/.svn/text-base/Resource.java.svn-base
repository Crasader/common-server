package cn.com.kanjian.model;

import java.io.Serializable;
import java.util.Date;

public class Resource implements Serializable {

	private static final long serialVersionUID = 360861681101071145L;

	private Long id;
	private String title;
	private String content;
	private Long duration;
	private Integer restype;
	private String des;
	private String xpic;
	private String hpic;
	private Long author;
	private Integer isdel;
	private Date publishtime;
	private Date intime;

	public Resource() {
		
	}
	
	public Resource(String title, String content, Long duration,
			Integer restype, String des, String xpic, String hpic,
			Long author, Integer isdel, Date publishtime, Date intime) {
		super();
		this.title = title;
		this.content = content;
		this.duration = duration;
		this.restype = restype;
		this.des = des;
		this.xpic = xpic;
		this.hpic = hpic;
		this.author = author;
		this.isdel = isdel;
		this.publishtime = publishtime;
		this.intime = intime;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Long getDuration() {
		return duration;
	}

	public void setDuration(Long duration) {
		this.duration = duration;
	}

	public Integer getRestype() {
		return restype;
	}

	public void setRestype(Integer restype) {
		this.restype = restype;
	}

	public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}

	public String getXpic() {
		return xpic;
	}

	public void setXpic(String xpic) {
		this.xpic = xpic;
	}

	public String getHpic() {
		return hpic;
	}

	public void setHpic(String hpic) {
		this.hpic = hpic;
	}

	public Long getAuthor() {
		return author;
	}

	public void setAuthor(Long author) {
		this.author = author;
	}

	public Integer getIsdel() {
		return isdel;
	}

	public void setIsdel(Integer isdel) {
		this.isdel = isdel;
	}

	public Date getIntime() {
		return intime;
	}

	public void setIntime(Date intime) {
		this.intime = intime;
	}

	public Date getPublishtime() {
		return publishtime;
	}

	public void setPublishtime(Date publishtime) {
		this.publishtime = publishtime;
	}

}
